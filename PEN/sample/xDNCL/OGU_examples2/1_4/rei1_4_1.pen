gOpenWindow(200, 200)	    /* 200x200のウィンドウを開く */
gDrawBox(20, 40, 160, 120)  /* (20, 40)に 160x120の矩形  */
gSetFillColor(0,255,0)      /* 塗りつぶしの色を緑に設定  */
gFillCircle(100,100,40)     /* 中心(100,100)半径40の円   */
gSetTextColor(255, 0, 0)    /* 文字の色を赤色に設定      */
gSetFontSize(24)            /* フォントサイズを24に設定  */
gDrawText("例題", 80, 190)  /* (80,190)に文字を描画 */